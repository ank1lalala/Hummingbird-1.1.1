{
	"manifest_version": 2,
	"name": "Hummingbird",
	"author": "KateLockwood&ANKlalala",
	"description": "This is extension which helps to look through the sites, and mark suitable ones in the google spreadsheets",
	"version": "1.1.1",
	"icons": {
		"128": "icon.png"
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"background": {
		"persistent": true,
		"scripts": ["background.js"]
	},
	"permissions": [
		"activeTab",
		"tabs",
		"background"
	],

	"commands": {
		"result1-event": {
			"suggested_key": {
				"default": "Alt+A",
				"mac": "Command+Shift+Y"
			},
			"description": "Execute Result1"
		},
		"result2-event": {
			"suggested_key": {
				"default": "Alt+A",
				"mac": "Command+Shift+Y"
			},
			"description": "Execute Result2"
		},
		"parser-event": {
			"suggested_key": {
				"default": "Alt+A",
				"mac": "Command+Shift+Y"
			},
			"description": "Execute Parser"
		}
	}
}